<div class="container">
    <div class="register">
        <h2>Register</h2>
            <div class=" col-md-6 register-bottom-grid">

                <form action="@Url.Action("RegistrarCliente","Home")" method="post">
                    <div class="form-group">
                        <input type="text" class="form-control" id="nombres-modal" name="Nombres" placeholder="Nombres">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" id="apellidos-modal" name="Apellidos" placeholder="Apellidos">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" id="usuario-modal" name="Usuario" placeholder="Usuario">
                    </div>
                    <div class="form-group">
                        <input type="password" class="form-control" id="clave-modal" name="Clave" placeholder="Password">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" id="correo-modal" name="Correo" placeholder="Email">
                    </div>


                    <p class="text-center">
                        <button type="submit" class="btn btn-primary"><i class="fa fa-sign-in"></i>Registarse</button>
                    </p>

                </form>
            </div>
            <div class="clearfix"> </div>
    </div>
</div> 

